int waitForZnew() {
	// print("hereZ");
	mmc.setSerialPortCommand("COM4","!statusaxis","\r"); 
	status = mmc.getSerialPortAnswer("COM4","\r"); 
	while(status.charAt(0) == 'M' || status.charAt(1) == 'M') {
		mmc.setSerialPortCommand("COM4","!statusaxis","\r"); 
		status = mmc.getSerialPortAnswer("COM4","\r"); 
		Thread.sleep(100);  // 10 Hz polling rate
	}	
	return 0;
}

